{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc"]
    },
    "source": {
      "include": [
        "server/modules/users"
      ],
      "includePattern": "\\.(js)$",
      "excludePattern": "(node_modules/|docs)"
    },
    "opts": {
      "destination": "./docs",
      "recurse": true,
      "readme": "./server/Readme.md"
    },
    "templates": {
      "default": {
        "outputSourceFiles": true
      }
    }
  }
  